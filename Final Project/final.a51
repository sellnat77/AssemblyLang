		ORG 0000H
		LJMP MAIN

DATAB:	
		DB 17H,37H,9H,18H,11H,26H,23H,43H
MAIN:	
		MOV R0,#40H
		MOV DPTR,#DATAB
		MOV R7,#08H
			
LOOP:		
		CLR A	
		MOVC A,@A+DPTR
		MOV @R0,A
		INC R0
		INC DPTR
		DJNZ R7,LOOP
		ACALL PACK
		SJMP MAIN
		
PACK:		
		MOV R0,#40H
		MOV R7,#08H
AGAIN:		
		MOV A,@R0
		MOV B,#10H
		DIV AB
		SWAP A
		ORL A,B
		MOV @R0,10H
		
		INC A
		INC R0
		
		
		DJNZ R7,AGAIN
		RET
		END